---
import Base from "../../layouts/Base.astro";
const title = "Metabase for beginners";
const description =
  "Join our beginner-friendly event to learn how to set up Metabase, connect data sources, build queries, and create interactive dashboards.";
const seo = {
  title: "Metabase for beginners",
  description:
    "Join our beginner-friendly event to learn how to set up Metabase, connect data sources, build queries, and create interactive dashboards.",
  canonical: "/events/metabase-for-beginners",
  og: {
    title: "Metabase for beginners",
    description:
      "Join our beginner-friendly event to learn how to set up Metabase, connect data sources, build queries, and create interactive dashboards.",
    image: "https://www.metabase.com/images/events/metabase-for-beginners.jpg",
    url: "https://www.metabase.com/events/metabase-for-beginners",
    type: "article",
    site_name:
      "Metabase | Open source Business Intelligence and Embedded Analytics",
  },
  twitter: {
    site: "@metabase",
    title: "Metabase for beginners",
    description:
      "Join our beginner-friendly event to learn how to set up Metabase, connect data sources, build queries, and create interactive dashboards.",
    card: "summary_large_image",
    image: "https://www.metabase.com/images/events/metabase-for-beginners.jpg",
  },
};
