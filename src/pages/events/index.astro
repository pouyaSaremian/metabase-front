---
import Base from "../../layouts/Base.astro";
const title = "Events";
const description =
  "Find upcoming Metabase events, webinars, and workshops to learn, connect, and enhance your analytics skills.";
const seo = {
  title: "Events",
  description:
    "Find upcoming Metabase events, webinars, and workshops to learn, connect, and enhance your analytics skills.",
  canonical: "/events/",
  og: {
    title: "Events",
    description:
      "Find upcoming Metabase events, webinars, and workshops to learn, connect, and enhance your analytics skills.",
    url: "https://www.metabase.com/events",
    type: "article",
    site_name:
      "Metabase | Open source Business Intelligence and Embedded Analytics",
  },
  twitter: {
    site: "@metabase",
    title: "Events",
    description:
      "Find upcoming Metabase events, webinars, and workshops to learn, connect, and enhance your analytics skills.",
    card: "summary",
    image: "https://www.metabase.com/images/twitter/default-share.png",
  },
};
