---
import Base from "../layouts/Base.astro";
const title = "How companies use Metabase";
const description =
  "Learn how companies across industries use Metabase to simplify data analysis, implement customer-facing analytics fast, and empower teams with actionable insights.";
const seo = {
  title: "How companies use Metabase",
  description:
    "Learn how companies across industries use Metabase to simplify data analysis, implement customer-facing analytics fast, and empower teams with actionable insights.",
  canonical: "/case-studies",
  og: {
    title: "How companies use Metabase",
    description:
      "Learn how companies across industries use Metabase to simplify data analysis, implement customer-facing analytics fast, and empower teams with actionable insights.",
    image:
      "https://www.metabase.com/images/case-studies/case-studies-main-og.jpg",
    url: "https://www.metabase.com/case-studies",
    type: "article",
    site_name:
      "Metabase | Open source Business Intelligence and Embedded Analytics",
  },
  twitter: {
    site: "@metabase",
    title: "How companies use Metabase",
    description:
      "Learn how companies across industries use Metabase to simplify data analysis, implement customer-facing analytics fast, and empower teams with actionable insights.",
    card: "summary_large_image",
    image:
      "https://www.metabase.com/images/case-studies/case-studies-main-og.jpg",
  },
};
