---
import Base from "../../layouts/Base.astro";
const title = "Self-service Business Intelligence | Metabase";
const description =
  "Self-service Business Intelligence. Empower your team to leverage data and dashboards using Metabase.";
const seo = {
  title: "Self-service Business Intelligence | Metabase",
  description:
    "Self-service Business Intelligence. Empower your team to leverage data and dashboards using Metabase.",
  canonical: "/product/business-intelligence",
  og: {
    title: "Self-service Business Intelligence | Metabase",
    description:
      "Self-service Business Intelligence. Empower your team to leverage data and dashboards using Metabase.",
    image: "https://www.metabase.com/images/product/business-analytics.jpg",
    url: "https://www.metabase.com/product/business-intelligence",
    type: "article",
    site_name:
      "Metabase | Open source Business Intelligence and Embedded Analytics",
  },
  twitter: {
    site: "@metabase",
    title: "Self-service Business Intelligence | Metabase",
    description:
      "Self-service Business Intelligence. Empower your team to leverage data and dashboards using Metabase.",
    card: "summary_large_image",
    image: "https://www.metabase.com/images/product/business-analytics.jpg",
  },
};
