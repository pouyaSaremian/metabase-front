---
import Base from "../../layouts/Base.astro";
const title = "Onboarding professional service";
const description =
  "Get customer-facing analytics really fast and easy with hands-on help from the experts.";
const seo = {
  title: "Onboarding professional service",
  description:
    "Get customer-facing analytics really fast and easy with hands-on help from the experts.",
  canonical: "/product/onboarding-professional-service",
  og: {
    title: "Onboarding professional service",
    description:
      "Get customer-facing analytics really fast and easy with hands-on help from the experts.",
    image: "https://www.metabase.com/images/features/query-builder-og.jpg",
    url: "https://www.metabase.com/product/onboarding-professional-service",
    type: "article",
    site_name:
      "Metabase | Open source Business Intelligence and Embedded Analytics",
  },
  twitter: {
    site: "@metabase",
    title: "Onboarding professional service",
    description:
      "Get customer-facing analytics really fast and easy with hands-on help from the experts.",
    card: "summary_large_image",
    image: "https://www.metabase.com/images/features/query-builder-og.jpg",
  },
};
