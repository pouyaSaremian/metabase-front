---
import Base from "../../layouts/Base.astro";
const title = "Metabase for small teams";
const description =
  "Small business analytics tools. Leverage powerful business analytics tools that empower small teams with actionable insights.";
const seo = {
  title: "Metabase for small teams",
  description:
    "Small business analytics tools. Leverage powerful business analytics tools that empower small teams with actionable insights.",
  canonical: "/product/starter",
  og: {
    title: "Metabase for small teams",
    description:
      "Small business analytics tools. Leverage powerful business analytics tools that empower small teams with actionable insights.",
    image:
      "https://www.metabase.com/images/opengraph/metabase-starter-plan.jpg",
    url: "https://www.metabase.com/product/starter",
    type: "article",
    site_name:
      "Metabase | Open source Business Intelligence and Embedded Analytics",
  },
  twitter: {
    site: "@metabase",
    title: "Metabase for small teams",
    description:
      "Small business analytics tools. Leverage powerful business analytics tools that empower small teams with actionable insights.",
    card: "summary_large_image",
    image:
      "https://www.metabase.com/images/opengraph/metabase-starter-plan.jpg",
  },
};
---

<Base {title} {description} {seo}>
  <div class="no-overflow">
    <div class="bootstrap">
      <div class="responsive-page starter-page">
        <div class="hero-container">
          <div class="hero">
            <div id="gradient-background"></div>

            <div id="cloud-cover-left"></div>

            <div id="cloud-cover-right"></div>

            <div class="cloud" id="cloud-1"></div>
            <div class="cloud" id="cloud-2"></div>
            <div class="cloud" id="cloud-3"></div>
            <div class="cloud" id="cloud-4"></div>

            <div class="d-flex flex-column align-items-center">
              <h6>Metabase Starter</h6>
              <h1 class="fw-bold mb-5 text-center h3">
                All the goodness of Metabase,<br />minus all the hosting work
              </h1>
              <p
                class="paragraph-4 responsive neutral-40 mb-6 text-center subtitle"
              >
                Starter has all the stuff you get in the open source version of
                Metabase, with hosting taken care of. And you get hands-on
                support from our expert customer success engineers.
              </p>
            </div>
            <div class="d-flex justify-content-center align-items-center gap-4">
              <a
                id="get-started-btn-home"
                class="fs-13 Button btn btn-primary rounded-3"
                href="https://store.metabase.com/checkout">Get started</a
              >
              <a
                class="fs-13 Button btn btn-outline-blue rounded-3"
                href="/talk-to-a-person/">Contact us</a
              >
            </div>

            <div class="cards-container">
              <div class="card">
                <svg
                  xmlns="http://www.w3.org/2000/svg"
                  width="24"
                  height="24"
                  fill="none"
                  ><path
                    stroke="#80B946"
                    stroke-linecap="round"
                    stroke-linejoin="round"
                    stroke-width="1.5"
                    d="m8 15.86-3.25 2.39V5.75L8 8.14M19.25 12l-8.5-6.25v12.5l8.5-6.25Z"
                  ></path></svg
                >

                <h4>Get started in minutes</h4>
                <p>
                  Plug into your <a href="/data-sources/">data source</a>,
                  invite your team, and start asking questions.
                </p>
              </div>
              <div class="card">
                <svg
                  width="24"
                  height="24"
                  viewBox="0 0 24 24"
                  fill="none"
                  xmlns="http://www.w3.org/2000/svg"
