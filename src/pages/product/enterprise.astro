---
import Base from "../../layouts/Base.astro";
const title = "Enterprise Analytics and Business Intelligence Tools | Metabase";
const description =
  "Enterprise analytics and business intelligence tools. Metabase provides best-in-class business analytics and intelligence tools that helps drive data-based decisions at scale.";
const seo = {
  title: "Enterprise Analytics and Business Intelligence Tools | Metabase",
  description:
    "Enterprise analytics and business intelligence tools. Metabase provides best-in-class business analytics and intelligence tools that helps drive data-based decisions at scale.",
  canonical: "/product/enterprise",
  og: {
    title: "Enterprise Analytics and Business Intelligence Tools | Metabase",
    description:
      "Enterprise analytics and business intelligence tools. Metabase provides best-in-class business analytics and intelligence tools that helps drive data-based decisions at scale.",
    image:
      "https://www.metabase.com/images/opengraph/metabase-enterprise-plan.jpg",
    url: "https://www.metabase.com/product/enterprise",
    type: "article",
    site_name:
      "Metabase | Open source Business Intelligence and Embedded Analytics",
  },
  twitter: {
    site: "@metabase",
    title: "Enterprise Analytics and Business Intelligence Tools | Metabase",
    description:
      "Enterprise analytics and business intelligence tools. Metabase provides best-in-class business analytics and intelligence tools that helps drive data-based decisions at scale.",
    card: "summary_large_image",
    image:
      "https://www.metabase.com/images/opengraph/metabase-enterprise-plan.jpg",
  },
};
