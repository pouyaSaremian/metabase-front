        <div class="product-intro">
          <div class="container py4 mt4 mobile-top-48">
            <div class="feature-plus-headline-container">
              <div>
                <h2>Integrated with your UI</h2>
                <p>
                  Embed standalone components—from individual charts to the
                  query builder—that blend into your product. Pixel-perfect
                  placement with no fixed width or height for responsive
                  designs. Override menus, rearrange fields, move buttons, and
                  lay out components to fit your UI.
                </p>
              </div>
              <div class="feature-container">
                <div
                  class="integrate-with-ui-illustration bleed-image-to-right"
                >
                  <img
                    alt="Integrate With Ui 1"
                    class="no-shadow"
                    src="/images/product/embedded-analytics-sdk/integrate-with-ui-1.png"
                    style="margin: 0 -20px -60px 0"
                    loading="lazy"
                  />
                </div>
                <div class="language-javascript">
                  <div class="highlight">
                    <pre
                      class="highlight">        &lt;code class="language-javascript do-not-display-copy-button"&gt;
import &amp;#123;
  MetabaseProvider,
  InteractiveQuestion,
  defineEmbeddingSdkConfig
&amp;#125; from "@metabase/embedding-sdk-react";

const config = defineEmbeddingSdkConfig(&amp;#123;
  metabaseInstanceUrl: "https://metabase.example.com", // Required: Your Metabase instance URL
  authProviderUri: "https://app.example.com/sso/metabase", // Required: An endpoint in your app that signs the user in and returns a session
&amp;#125;);

&lt;MetabaseProvider config=&amp;#123;config&amp;#125;&gt;
  &lt;InteractiveQuestion
    questionId=&amp;#123;questionId&amp;#125;
    isSaveEnabled=&amp;#123;false&amp;#125;
    withResetButton=&amp;#123;true&amp;#125;
    customTitle="Orders over time"
  /&gt;
&lt;/MetabaseProvider&gt;
        &lt;/code&gt;
      </pre>
