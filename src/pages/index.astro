---
import Base from "../layouts/Base.astro";
---

<Base title="Home" description="Open source analytics, deployed fast">
  <div class="no-overflow">
    <div class="bootstrap">
      <div class="bg-neutral-98-gradient--gradual">
        <div class="home-page">
          <div class="container pt-8 pt-sm-13">
            <div class="d-flex flex-column align-items-center">
              <h1 class="fw-bold mb-5 text-center h3">
                Open source analytics, deployed fast
              </h1>
              <div class="row justify-content-center">
                <div class="col-xs-12 col-md-10 col-lg-8">
                  <p
                    class="paragraph-4 responsive neutral-40 mb-6 text-center"
                    style="max-width: 840px"
                  >
                    برای هوش تجاری و تجزیه و تحلیل‌های جاسازی‌شده به پایگاه داده
                    خود متصل شوید. بدون تیم پیاده‌سازی. بدون آموزش طولانی. بدون
                    دردسر راه‌اندازی یا هزینه‌های پنهان. فقط گزارش‌هایی که
                    کاربران تجاری شما می‌توانند اجرا کنند. بدون نیاز به SQL.
                  </p>
                </div>
              </div>
            </div>
            <div class="d-flex justify-content-center gap-4">
              <a
                id="get-started-btn-home"
                class="fs-13 Button btn btn-primary rounded-3 get-started-btn-home-vwo"
                href="https://store.metabase.com/checkout"
              >
                Try Metabase Cloud free
              </a>
              <a
                class="fs-13 Button btn btn-outline-blue rounded-3 watch-demo-btn-home-vwo"
                href="start/oss/index.html"
              >
                Deploy Metabase Open Source
              </a>
            </div>
            <div class="d-flex flex-column align-items-center">
              <div class="row justify-content-center">
                <div
                  class="d-flex col-xs-12 justify-content-center align-items-center pt-9 pb-3 relative"
                >
                  <img alt="Curved Arrow" src="images/icons/curved-arrow.svg" />
                  <p
                    class="mb-0 ml-2 neutral-40 pb-3 paragraph-5-5 paragraph--mobile__smaller fw-bold"
                  >
                    See how to go from zero to dashboard in 3 minutes
                  </p>
                </div>
              </div>
            </div>
          </div>

          <div
            class="my-11 mt-0 d-flex align-items-center justify-content-center dashboard--preview relative"
          >
            <div
              id="stats-dashboard-background-img-wrapper"
              class="position-absolute"
            >
              <img
                id="stats-dashboard-background-img"
                src="images/home/illustration-2.svg"
                alt="Bridge"
              />
              <div id="stats-dashboard-background-img-gradient"></div>
            </div>
            <div class="container d-flex justify-content-center">
              <div class="row justify-content-center">
                <div
                  class="col-xs-12 p-0 relative video--hero relative overflow-hidden w-100"
                >
                  <a
                    href="demo.html"
                    class="absolute m-auto start-0 end-0 button--playDemo"
                  >
                    <svg
                      id=""
                      class=""
                      width="33"
                      height="33"
                      viewBox="0 0 10 12"
                      fill="white"
                    >
                      <path
                        d="M9.0625 5.02572C9.8125 5.45873 9.8125 6.54127 9.0625 6.97428L2.3125 10.8714C1.5625 11.3044 0.625001 10.7631 0.625001 9.89711L0.625002 2.10289C0.625002 1.23686 1.5625 0.695594 2.3125 1.12861L9.0625 5.02572Z"
                      ></path>
                    </svg>
                  </a>
                  <div class="video--heroVideoContainer">
                    <video
                      class="video--heroVideo vjs-player pause-observer lazy"
                      id="hero-video"
                      poster="images/home/homepage_video.gif"
                    >
                      <source
                        src="images/home/homepage_video.mp4"
                        type="video/mp4"
                      />
                      <source
                        src="images/home/homepage_video.webm"
                        type="video/webm"
                      />
                      <p class="vjs-no-js">
                        To view this video please enable JavaScript and consider
                        upgrading to a web browser that
                        <a
                          href="https://videojs.com/html5-video-support/"
                          target="_blank">supports HTML5 video</a
                        >
                      </p>
                    </video>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="d-flex flex-row justify-content-center">
            <p class="paragraph-4-5 neutral-40 my-0 mb-6 py-0">
              Metabase is trusted by over 80,000 companies
            </p>
