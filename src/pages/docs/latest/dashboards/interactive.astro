    url: "https://www.metabase.com/docs/latest/dashboards/interactive",
    type: "article",
    site_name:
      "Metabase | Business Intelligence, Dashboards, and Data Visualization",
  },
  twitter: {
    site: "@metabase",
    title: "Dashboard interactivity | Metabase Documentation",
    description:
