const title = "Dashboard interactivity | Metabase Documentation";
const description =
  "Dashboard interactivity You can customize what happens when people click on questions in your dashboard. By default, when you create charts using Metabase’s graphical query builder, your charts automatically come with drill-through capabilities, which let folks click on a chart...";
