import Base from "../../../../../layouts/Base.astro";
const title = "Pivot tables | Metabase Documentation";
const description =
  "Pivot tables  Pivot tables are currently only supported for questions built in the query builder. Pivot tables are not supported for NoSQL databases like MongoDB.  Pivot tables allow you swap rows and columns, group data, and include subtotals...";
const seo = {
  title: "Pivot tables | Metabase Documentation",
  description:
    "Pivot tables  Pivot tables are currently only supported for questions built in the query builder. Pivot tables are not supported for NoSQL databases like MongoDB.  Pivot tables allow you swap rows and columns, group data, and include subtotals...",
  canonical: "/docs/latest/questions/visualizations/pivot-table",
  og: {
    title: "Pivot tables | Metabase Documentation",
    description:
      "Pivot tables  Pivot tables are currently only supported for questions built in the query builder. Pivot tables are not supported for NoSQL databases like MongoDB.  Pivot tables allow you swap rows and columns, group data, and include subtotals...",
    image: "https://www.metabase.com/images/docs/docs-og.png",
    url: "https://www.metabase.com/docs/latest/questions/visualizations/pivot-table",
    type: "article",
    site_name:
      "Metabase | Business Intelligence, Dashboards, and Data Visualization",
  },
  twitter: {
    site: "@metabase",
    title: "Pivot tables | Metabase Documentation",
    description:
      "Pivot tables  Pivot tables are currently only supported for questions built in the query builder. Pivot tables are not supported for NoSQL databases like MongoDB.  Pivot tables allow you swap rows and columns, group data, and include subtotals...",
    card: "summary_large_image",
    image: "https://www.metabase.com/images/docs/docs-og.png",
  },
};
