---
import Base from "../../../../layouts/Base.astro";
const title = "Syncing and scanning databases | Metabase Documentation";
const description =
  "Syncing and scanning databases Metabase periodically runs different types of queries on your data warehouse to stay up to date with your database’s metadata. Knowing information about your data helps Metabase do things like display the right chart for the results...";
const seo = {
  title: "Syncing and scanning databases | Metabase Documentation",
  description:
    "Syncing and scanning databases Metabase periodically runs different types of queries on your data warehouse to stay up to date with your database’s metadata. Knowing information about your data helps Metabase do things like display the right chart for the results...",
  canonical: "/docs/latest/databases/sync-scan",
  og: {
    title: "Syncing and scanning databases | Metabase Documentation",
    description:
      "Syncing and scanning databases Metabase periodically runs different types of queries on your data warehouse to stay up to date with your database’s metadata. Knowing information about your data helps Metabase do things like display the right chart for the results...",
    image: "https://www.metabase.com/images/docs/docs-og.png",
    url: "https://www.metabase.com/docs/latest/databases/sync-scan",
    type: "article",
    site_name:
      "Metabase | Business Intelligence, Dashboards, and Data Visualization",
  },
  twitter: {
    site: "@metabase",
    title: "Syncing and scanning databases | Metabase Documentation",
    description:
      "Syncing and scanning databases Metabase periodically runs different types of queries on your data warehouse to stay up to date with your database’s metadata. Knowing information about your data helps Metabase do things like display the right chart for the results...",
    card: "summary_large_image",
    image: "https://www.metabase.com/images/docs/docs-og.png",
  },
};
