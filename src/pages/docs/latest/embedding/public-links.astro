import Base from "../../../../layouts/Base.astro";
const title = "Public sharing | Metabase Documentation";
const description =
  "Public sharing  Only admins can create public links and iframes.  Admins can create and share public links (URLs) for questions and dashboards. People can view them as standalone destinations (URLs) or as embedded iframes in another page or app....";
const seo = {
  title: "Public sharing | Metabase Documentation",
  description:
    "Public sharing  Only admins can create public links and iframes.  Admins can create and share public links (URLs) for questions and dashboards. People can view them as standalone destinations (URLs) or as embedded iframes in another page or app....",
  canonical: "/docs/latest/embedding/public-links",
