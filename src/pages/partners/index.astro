  title: "Get Help with Metabase from one of our Experts",
  description:
    "Discover Metabase partners who offer solutions, integrations, and services to enhance your analytics experience.",
  canonical: "/partners/",
  og: {
    title: "Get Help with Metabase from one of our Experts",
    description:
      "Discover Metabase partners who offer solutions, integrations, and services to enhance your analytics experience.",
    image: "https://www.metabase.com/images/opengraph/_partners/og-experts.jpg",
    url: "https://www.metabase.com/partners",
    type: "article",
    site_name:
      "Metabase | Open source Business Intelligence and Embedded Analytics",
  },
  twitter: {
    site: "@metabase",
    title: "Get Help with Metabase from one of our Experts",
    description:
      "Discover Metabase partners who offer solutions, integrations, and services to enhance your analytics experience.",
