---
import Base from "../layouts/Base.astro";
const title = "Metabase | See what’s possible with embedded analytics";
const description =
  "Take a live demo for a spin and see what you can build with embedded analytics.";
const seo = {
  title: "Metabase | See what’s possible with embedded analytics",
  description:
    "Take a live demo for a spin and see what you can build with embedded analytics.",
  canonical: "/embedding-demo",
  og: {
    title: "Metabase | See what’s possible with embedded analytics",
    description:
      "Take a live demo for a spin and see what you can build with embedded analytics.",
    image: "https://www.metabase.com/images/opengraph/embedding-demo-og.jpg",
    url: "https://www.metabase.com/embedding-demo",
    type: "article",
    site_name:
      "Metabase | Open source Business Intelligence and Embedded Analytics",
  },
  twitter: {
    site: "@metabase",
    title: "Metabase | See what’s possible with embedded analytics",
    description:
      "Take a live demo for a spin and see what you can build with embedded analytics.",
    card: "summary_large_image",
    image: "https://www.metabase.com/images/opengraph/embedding-demo-og.jpg",
  },
};
